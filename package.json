{
  "name": "war-craft-shopping",
  "version": "1.0.0",
  "main": "server/index.js",
  "dependencies": {
    "axios": "^0.19.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.12.1",
    "next": "^11.1.3",
    "next-redux-wrapper": "^2.1.0",
    "next-routes": "^1.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.7.1",
    "lint-staged": "^9.0.0",
    "nodemon": "^1.12.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "standard": "^13.0.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "build": "NODE_ENV=production next build",
    "test:ci": "jest --maxWorkers=2 --ci",
    "test": "jest --watch",
    "start": "nodemon -w server server/index.js",
    "serve": "NODE_ENV=production node server/index.js",
    "clean": "rimraf node_modules/.cache .next",
    "lint": "standard --verbose | snazzy",
    "now-start": "yarn serve",
    "lint:fix": "standard --fix --verbose | snazzy"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 8.12"
  },
  "jest": {
    "modulePaths": [
      "./src",
      "./src/test"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "@testing-library/react/cleanup-after-each"
    ]
  },
  "author": "Rajath",
  "license": "ISC",
  "description": "Watto's Emporium"
}
